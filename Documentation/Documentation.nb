 CellChangeTimes->{{3.580158523920807*^9, 
  3.580158525608903*^9}},ExpressionUUID->"be64c14b-f2c6-49c4-8478-\
7ef7350be4d6"],

Cell[TextData[{
 "A series ",
 StyleBox["c",
  FontSlant->"Italic"],
 " \[Element] ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["\[DoubleStruckCapitalR]", "\[ScriptL]"], TraditionalForm]],
  ExpressionUUID->"ce390406-f35a-4317-951a-2a2c49a383f9"],
 "\[LeftAngleBracket]\[LeftAngleBracket]",
 StyleBox["X",
  FontSlant->"Italic"],
 "\[RightAngleBracket]\[RightAngleBracket] is said to be ",
 StyleBox["globally convergent",
  FontSlant->"Italic"],
 " if there exists real numbers ",
 StyleBox["K",
  FontSlant->"Italic"],
 ",",
 StyleBox["M",
  FontSlant->"Italic"],
 " > 0 such that |(",
 StyleBox["c",
  FontSlant->"Italic"],
 ", \[Eta])| \[LessEqual] ",
 StyleBox["K",
  FontSlant->"Italic"],
 Cell[BoxData[
  FormBox[
   SuperscriptBox["M", 
    RowBox[{"\[LeftBracketingBar]", "\[Eta]", "\[RightBracketingBar]"}]], 
   TraditionalForm]],ExpressionUUID->"559b4465-d205-45e0-ba51-cce1506551e9"],
 " for all \[Eta] \[Element] ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["X", "*"], TraditionalForm]],ExpressionUUID->
  "8fa86e55-1474-47a5-94fe-5e03a32289e0"],
 ", where |",
 StyleBox["z",
  FontSlant->"Italic"],
 "| = ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["max", "i"], TraditionalForm]],ExpressionUUID->
  "2e69036c-9b6d-45a2-851e-87d12eaf1cd8"],
 " |",
 Cell[BoxData[
  FormBox[
   SubscriptBox["z", "i"], TraditionalForm]],ExpressionUUID->
  "c33cb985-684b-4f0c-b098-4dbc49197185"],
 "| when ",
 StyleBox["z",
  FontSlant->"Italic"],
 " \[Element] ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["\[DoubleStruckCapitalR]", "\[ScriptL]"], TraditionalForm]],
  ExpressionUUID->"d5c9dadb-4740-45fb-b58e-284384bb52c1"],
 ", and |\[Eta]| denotes the length of the word \[Eta]. The smallest such \
constants are called the ",
 StyleBox["global growth constants",
  FontSlant->"Italic"],
 ". When possible, this command returns the minimal ",
 StyleBox["K",
  FontSlant->"Italic"],
 ",",
 StyleBox["M",
  FontSlant->"Italic"],
 " (in this order) and plots log(|(",
 StyleBox["c",
  FontSlant->"Italic"],
 ", \[Eta])|) versus word length, as well as the uniform bound log(",
 StyleBox["M",
  FontSlant->"Italic"],
 ")|\[Eta]| + log(",
 StyleBox["K",
  FontSlant->"Italic"],
 ").\n\nAssume there exists an expression such that |(",
 StyleBox["c",
  FontSlant->"Italic"],
 ", \[Eta])| \[LessEqual] ",
 StyleBox["K",
  FontSlant->"Italic"],
 Cell[BoxData[
  FormBox[
   SubsuperscriptBox["M", "0", 
    SubscriptBox[
     RowBox[{"\[LeftBracketingBar]", "\[Eta]", "\[RightBracketingBar]"}], 
     SubscriptBox["x", "0"]]], TraditionalForm]],ExpressionUUID->
  "39b68bda-24cb-48a1-b865-2f7082656fd3"],
 Cell[BoxData[
  FormBox[
   SubsuperscriptBox["M", "1", 
    SubscriptBox[
     RowBox[{"\[LeftBracketingBar]", "\[Eta]", "\[RightBracketingBar]"}], 
     SubscriptBox["x", "1"]]], TraditionalForm]],ExpressionUUID->
  "eeab4c28-ca4d-4d46-a443-4246ba36e4cb"],
 "\[CenterEllipsis]",
 Cell[BoxData[
  FormBox[
   SubsuperscriptBox["M", "m", 
    SubscriptBox[
     RowBox[{"\[LeftBracketingBar]", "\[Eta]", "\[RightBracketingBar]"}], 
     SubscriptBox["x", "m"]]], TraditionalForm]],ExpressionUUID->
  "edc24ae8-d4ae-457d-b36f-41faf0494e28"],
 "for all \[Eta] \[Element] ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["X", "*"], TraditionalForm]],ExpressionUUID->
  "c9f6778c-d4a8-45d8-827b-ed7586f2a913"],
 " and ",
 StyleBox["X ",
  FontSlant->"Italic"],
 "= {",
 Cell[BoxData[
  FormBox[
   SubscriptBox["x", "0"], TraditionalForm]],ExpressionUUID->
  "515feba9-5286-4216-b06b-f58daf54e344"],
 ", ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["x", "1"], TraditionalForm]],ExpressionUUID->
  "13363a2b-23e5-48e5-b857-3a6157836d33"],
 ", \[Ellipsis], ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["x", "m"], TraditionalForm]],ExpressionUUID->
  "d671274e-4731-4fa1-a8b5-7069aa11ee1e"],
 "}. When including a list alphabet, the command returns the minimal ",
 StyleBox["K,",
  FontSlant->"Italic"],
 Cell[BoxData[
  FormBox[
   SubscriptBox["M", "0"], TraditionalForm]],ExpressionUUID->
  "c2f6d87c-c141-470c-9ad8-f89c1ea0fa3b"],
 StyleBox[",",
  FontSlant->"Italic"],
 Cell[BoxData[
  FormBox[
   SubscriptBox["M", "1"], TraditionalForm]],ExpressionUUID->
  "22d76b9a-c927-40d4-9f60-c31e3e036ee4"],
 StyleBox[",...,",
  FontSlant->"Italic"],
 Cell[BoxData[
  FormBox[
   SubscriptBox["M", "m"], TraditionalForm]],ExpressionUUID->
  "cb36a22d-d16a-4a10-9aec-12ffe656769e"],
 " (in this order) as well as showing a table of error values for the \
linearized fit of the form log(|(",
 StyleBox["c",
  FontSlant->"Italic"],
 ", \[Eta])|) = log(",
 StyleBox["K",
  FontSlant->"Italic"],
 ") + log(",
 Cell[BoxData[
  FormBox[
   SubscriptBox["M", "0"], TraditionalForm]],ExpressionUUID->
  "683da80f-e515-4083-b416-4d1d9f3be0c2"],
 ")",
 Cell[BoxData[
  FormBox[
   SubscriptBox[
    RowBox[{"\[LeftBracketingBar]", "\[Eta]", "\[RightBracketingBar]"}], 
    SubscriptBox["x", "0"]], TraditionalForm]],ExpressionUUID->
  "71f0e2e1-6a96-4209-baf4-838bb3d81c9b"],
 "+ log(",
 Cell[BoxData[
  FormBox[
   SubscriptBox["M", "1"], TraditionalForm]],ExpressionUUID->
  "e1a29e02-a0db-4d23-9257-c68582d36fac"],
 ")",
 Cell[BoxData[
  FormBox[
   SubscriptBox[
    RowBox[{"\[LeftBracketingBar]", "\[Eta]", "\[RightBracketingBar]"}], 
    SubscriptBox["x", "1"]], TraditionalForm]],ExpressionUUID->
  "8c68c3fc-0806-4ff6-a38c-d73c6e97b3b1"],
 "+ \[CenterEllipsis] + log(",
 Cell[BoxData[
  FormBox[
   SubscriptBox["M", "m"], TraditionalForm]],ExpressionUUID->
  "240d6d02-7d3a-46c0-ae2d-4ab319d3ce5c"],
 ")",
 Cell[BoxData[
  FormBox[
